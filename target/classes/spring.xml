<?xml version="1.0" encoding="UTF-8"?>
<beans  xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
    http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">
    
    <bean id="empobj" class="com.vivek.Employee">
    	<constructor-arg value="101" type="int"></constructor-arg>
    	<constructor-arg value="Vivek" type="String"></constructor-arg>
    	<constructor-arg value="10000" type="float"></constructor-arg>
    	<property name="id" value="11"></property>
    	<property name="name" value="Kumar"></property>
    	<property name="salary" value="15000"></property>
    </bean>
    <bean id="empobj1" class="com.vivek.Employee">
    	<property name="id" value="12"></property>
    	<property name="name" value="Jack"></property>
    	<property name="salary" value="14000"></property>
    </bean>
</beans>